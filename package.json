    "idb-keyval": "^6.2.0",
    "image-js": "^0.35.2",
    "mapbox-gl": "^2.10.0",
    "mitt": "^3.0.0",
    "quasar": "^2.9.2",
    "vue": "^3.2.40",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.1.2",
    "autoprefixer": "^10.4.12",
    "electron": "^21.1.0"
