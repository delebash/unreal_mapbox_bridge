    "mitt": "^3.0.0",
    "quasar": "^2.7.5",
    "vue": "^3.2.37",
    "vue-router": "^4.1.1",
    "@turf/turf": "^6.5.0",
    "idb-keyval": "^6.2.0",
    "image-js": "^0.34.1",
    "mapbox-gl": "^2.9.1",
    "@jayarjo/utm": "^1.1.3",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@mapbox/mapbox-sdk": "^0.13.4",
    "@mapbox/tilebelt": "^1.0.2",
    "utm-zone": "^1.0.1"
  },
  "devDependencies": {
    "@quasar/app-vite": "^1.0.5",
